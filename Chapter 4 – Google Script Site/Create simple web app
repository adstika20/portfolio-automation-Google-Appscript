// Create a simple web 
function doGet() {
  return HtmlService.createHtmlOutput('<h1>Hai nama saya adalah ades tikaningsih tinggal di paguyangan</h>')
}

// Google Apps Script Web App with Bootstrap View
function doGet() {
  var output = HtmlService.createHtmlOutputFromFile('index');
  return output;
}

